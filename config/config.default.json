{
  "ui": {
    "icons": { "size": 38, "image_size": 150 },
    "orientation": "vertical",
    "ignore_exclusive": true,
    "horizontal": "center",
    "vertical": "start",
    "width": 400,
    "margins": {
      "top": 0
    },
    "anchors": {
      "top": false
    }
  },
  "list": {
    "always_show": true,
    "fixed_height": false,
    "height": 300,
    "margin_top": 10,
    "max_entries": 50,
    "scrollbar_policy": "automatic",
    "show_initial_entries": true,
    "width": 400
  },
  "search": {
    "typeahead": true,
    "force_keyboard_focus": true,
    "icons": true,
    "history": true,
    "placeholder": "Search...",
    "delay": 0
  },
  "activation_mode": {},
  "builtins": {
    "applications": { "actions": true },
    "clipboard": {
      "image_height": 300,
      "max_entries": 10,
      "switcher_only": true
    },
    "commands": { "switcher_only": true },
    "emojis": { "switcher_only": true },
    "finder": { "switcher_only": true },
    "hyprland": { "context_aware_history": true },
    "runner": { "Excludes": null, "Includes": null },
    "ssh": { "switcher_only": true },
    "switcher": { "prefix": "/" },
    "websearch": {}
  }
}
